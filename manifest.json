{
  "manifest_version": 3,
  "name": "NetFree YouTube Checker",
  "description": "בודק סרטוני YouTube בעמוד הנוכחי מול הAPI של נטפרי ומציג רשימה של סרטונים: פתוחים, שלא נבדקו וחסומים.",
  "version": "1.0",
  "permissions": [
    "scripting",
    "activeTab",
    "windows",
    "tabs"
  ],
  "host_permissions": [
    "https://www.google.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}